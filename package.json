{
  "name": "react-pokedex",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start-e2e": "nx e2e web-e2e --watch",
    "start": "nx serve",
    "storybook": "nx run ui:storybook",
    "graph": "nx run graph:serve",
    "build": "npm run codegen && nx build",
    "test": "npm run test:ui && npm run test:e2e",
    "test:ui": "start-server-and-test storybook http://localhost:4400 test:ui-jest",
    "test:ui-jest": "cd ./libs/ui && npx jest",
    "test:ui-jest-update": "cd ./libs/ui && npx jest -u",
    "test:e2e": "start-server-and-test graph 'http-get://localhost:3333/graphql?query={ __schema { queryType { name } } }' 'nx run-many --target=e2e --all'",
    "codegen": "graphql-codegen --config apps/web/codegen.yml"
  },
  "private": true,
